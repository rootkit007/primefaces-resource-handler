<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
    <application>
    	<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    	<resource-handler>coop.irnet.web.resources.PrimeResourceHandler</resource-handler>
    </application>
    
	<render-kit>    
		<renderer>
	         <component-family>org.primefaces.component</component-family>
	         <renderer-type>org.primefaces.component.graphicimage.GraphicImageRenderer</renderer-type>
	         <renderer-class>com.greatnowhere.jsf.resources.GraphicImageRenderer</renderer-class>
	    </renderer>
		<renderer>
	         <component-family>org.primefaces.component</component-family>
	         <renderer-type>org.primefaces.component.GraphicImageRenderer</renderer-type>
	         <renderer-class>com.greatnowhere.jsf.resources.GraphicImageRenderer</renderer-class>
	    </renderer>
	</render-kit>

</faces-config>    
